<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>HTTP REQUEST</h1>
      <hr>
      <div class="row">
        <div class="col-md-12">
          <form id="form" action="/received" method="post">

            <div class="form-group">
              <label for="">Título</label>
              <input type="text" class="form-control" name="titulo" id="titulo" placeholder="Enter title">
            </div>

            <div class="form-group">
              <label for="conteudo">Conteúdo</label>
              <textarea name="conteudo" id="conteudo" cols="30" rows="10" class="form-control"></textarea>
            </div>

            <button type="submit" class="btn btn-primary mt-5">Cadastrar</button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
